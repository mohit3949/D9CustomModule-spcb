/**
 * Implements hook_theme().
 */
function spcb_theme() {
  $theme['custom_block'] = [
    'variables' => [
      'current_time' => NULL,
      'country' => NULL,
      'city' => NULL,
    ],
    'template' => 'custom-block',
  ];

  return $theme;
}



